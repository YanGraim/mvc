@model List<Professor>

<table class="table table-striped">
    <thead>
    <tr>
        <th scope="col">Disciplina</th>
        <th scope="col">Nome</th>
        <th scope="col">Cpf</th>
    </tr>
    </thead>
    <tbody>
    @if (Model != null && Model.Any())
    {
        foreach (Professor professor in Model)
        {
            <tr>
                <td>@professor.Disciplina </td>
                <td>@professor.Nome </td>
                <td>@professor.Cpf </td>
                <td>
                @*     <a type="submit" class="btn btn-primary" role="button" asp-controller="Aluno" asp-action="Editar" asp-route-id="@aluno.Id">Editar</a> *@
                @* </td> *@
                @* <td> *@
                @*     <a type="submit" class="btn btn-primary" role="button" asp-controller="Aluno" asp-action="DeletarAluno" asp-route-id="@aluno.Id">Deletar</a> *@
                </td>
            </tr>
        }
    }
    </tbody>
</table>

<div class="text-center">
    <a class="btn btn-primary" type="button" asp-controller="Professor" asp-action="AdicionarProfessor"> Adicionar Professor </a>
</div>